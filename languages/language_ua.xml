<?xml version="1.0" encoding="UTF-8"?>
<data>
  <!-- text for main window -->
  <main_window>
      <!-- this if name program -->
    <name_program>Vocabulary_English</name_program>
    <!-- description buttons-->
    <label_button_window>
        <!-- button what stop session-->
      <button_pause>Пауза</button_pause>
      <!-- button what start session-->
      <button_start>Старт</button_start>
      <!-- button what check word-->
      <button_check>Перевірка</button_check>
      <!-- button view definition word-->
      <button_definition>Дефініція</button_definition>
      <!-- button voice word-->
      <button_voice>Озвучити текст</button_voice>
    </label_button_window>
    <!-- text on main window-->
    <label>
      <!-- Suggestions for the current word-->
      <help_for_word>Підказки:</help_for_word>
      <!-- Stopwatch session-->
      <timer> hh:mm:ss</timer>
    </label>
  </main_window>
  <!-- Help window, which explains the characters in the program-->
  <helper_window>
    <!-- Help window name -->
    <title>Допомога</title>
  </helper_window>
  <!-- setting window-->
  <setting_window>
    <!-- setting window name -->
    <title>Налаштування</title>
    <!-- column setting -->
    <title_column>
      <!-- column general setting -->
      <general_settings>
        <name>Головні налаштування</name>
         <!-- Two column. Description help-->
        <type_change>title</type_change>
      </general_settings>
    </title_column>
    <!-- name and type elements -->
    <elements_setting>
      <!-- language settings -->
    <language_interface>
        <!-- One column. Name help-->
        <name>Мова інтерфейсу</name>
         <!-- Two column. Description help-->
        <type_change>choice_area</type_change>
    </language_interface>
    </elements_setting>
    <!-- Name button -->
        <label_button_window>
      <!-- Exit button-->
      <button_exit>Вийти у головне меню</button_exit>
      <!-- save setting-->
      <button_voice>Сберегти налаштування</button_voice>
    </label_button_window>
  </setting_window>
  <!-- Bottom and top line of the program-->
  <bar>
    <!-- Help window prompt-->
    <label_help>F1 Допомога</label_help>
    <last_words>Залишилось:</last_words>
  </bar>
  <!-- Wrong translation information window-->
  <wrong_window>
    <!-- Wrong translation information window name-->
    <title_window>Вы помилились з перекладом цього слова</title_window>
    <!-- Text for Wrong translation information window  -->
    <label_table_wrong>
      <!-- Description wrong word -->
      <description_wrong_word>Ваш переклад</description_wrong_word>
      <!-- Stopwatch session-->
      <description_true_word_and_translate>Правильний переклад слова </description_true_word_and_translate>
      <!-- Description true word and translate-->
      <description_part_of_a_word>Частина слова</description_part_of_a_word>
      <!-- Description transcription-->
      <description_transcription>Транскрипція</description_transcription>
      <!-- Description definition-->
      <description_definition>Дефініція</description_definition>
      <!-- Description theme word-->
      <description_theme_word>Тема</description_theme_word>
    </label_table_wrong>
    <!-- description buttons-->
    <label_button_window>
      <!-- Exit button-->
      <button_exit>В головне меню</button_exit>
      <!-- button voice word-->
      <button_voice>Озвучити текст</button_voice>
    </label_button_window>
  </wrong_window>
  <!-- Window of victory-->
  <win_window>
    <!-- Text when all words are gone-->
    <label_win_text> Ви пройшли усі слова!</label_win_text>
  </win_window>
  <!-- Notifications-->
  <notifications>
    <!-- Exit Confirmation Window -->
    <exit_program>
      <!-- Title window-->
      <name>Вихід із програми</name>
      <!-- question for confirm exit-->
      <question>Ви хочете вийти з програми?</question>
      <!-- choice answer-->
      <choice>
        <!-- choice exit program-->
        <yes>Так</yes>
        <!-- choice back program -->
        <no>Ні</no>
      </choice>
     </exit_program>
    <!-- Definition output window-->
    <view_info_transcription>
      <!-- Definition output window name-->
      <title>Деініція</title>
      <!-- Text for definition output window-->
      <name_transcription>Дефініція</name_transcription>
    </view_info_transcription>
    <!-- text for help window-->
    <help>
      <!-- Input word-->
      <enter>
        <!-- One column. Name help-->
        <name>Enter</name>
         <!-- Two column. Description help-->
        <description>Перекласти слово</description>
      </enter>
      <!-- Status program/session-->
      <status_program>
        <!-- One column. Name help-->
        <name>F2</name>
        <!-- Two column. Description help-->
        <description>Почати/Остановити</description>
       </status_program>
      <!-- Transcription word-->
      <transcription>
        <!-- One column. Name help-->
        <name>⌥</name>
        <!-- Two column. Description help-->
        <description>Висновок транскрипції</description>
      </transcription>
      <!-- Voice word-->
      <voice>
        <!-- One column. Name help-->
        <name>⌘</name>
        <!-- Two column. Description help-->
        <description>Озвучення слова</description>
      </voice>
      <!-- Number of lives of a word in the current session -->
      <purple_heart>
        <!-- One column. Name help-->
        <name>:purple_heart:</name>
        <!-- Two column. Description help-->
        <description>Кількість життів слова у поточному сеансі. Коли життів 0, то слово вважається вивченим у поточному сеансі</description>
      </purple_heart>
      <!-- Number of sessions in a row where the word lost all lives in each session.-->
      <black_heart>
        <!-- One column. Name help-->
        <name>:black_heart:</name>
        <!-- Two column. Description help-->
        <description>Кількість сеансів поспіль, коли слово втрачало все життя у кожному сеансі. Коли три сеанси поспіль, то слово стає неактивним </description>
      </black_heart>
      <!-- Number of all words in programs-->
      <books>
       <!-- One column. Name help-->
        <name>:books:</name>
        <!-- Two column. Description help-->
        <description>Кількість усіх слів у програмі</description>
      </books>
      <!-- Number of words to learn-->
      <bullseye>
       <!-- One column. Name help-->
        <name>:bullseye:</name>
        <!-- Two column. Description help-->
        <description>Кількість слів, які потрібно вивчити (за винятком тимчасових та активних слів)</description>
      </bullseye>
      <!-- Number of inactive words-->
      <military_medal>
        <!-- One column. Name help-->
        <name>:military_medal:</name>
        <!-- Two column. Description help-->
        <description>Кількість неактивних слів -- Це слова, які вже були вивчені</description>
      </military_medal>
      <!-- Number of words that have already been repeated in the current session-->
      <check_mark_button>
        <!-- One column. Name help-->
        <name>:check_mark_button:</name>
       <!-- Two column. Description help-->
        <description>Кількість слів, які вже повторюються на поточному сеансі</description>
      </check_mark_button>
      <!--Number of words that have one life-->
      <keycap_1>
        <!-- One column. Name help-->
        <name>:keycap_1:</name>
        <!-- Two column. Description help-->
        <description>Кількість слів, що мають одне життя</description>
      </keycap_1>
      <!-- Number of words that have two life-->
      <keycap_2>
        <!-- One column. Name help-->
        <name>:keycap_2:</name>
        <!-- Two column. Description help-->
        <description>Кількість слів, що мають два життя</description>
      </keycap_2>
      <!-- Number of words that have three life-->
      <keycap_3>
        <!-- One column. Name help-->
        <name>:keycap_3:</name>
        <!-- Two column. Description help-->
        <description>Кількість слів, що мають три життя</description>
      </keycap_3>
    </help>
  </notifications>
    <!-- Data for menu bar-->
  <menu_bar>
    <!-- First tab. Main menu - place main function-->
    <tab_main_menu>
      <!-- name tab-->
      <name_tab>Головна</name_tab>
      <!-- elements tabs-->
      <rows_tab>
        <!-- element exit program-->
        <row_exit_program>Вихід</row_exit_program>
        <!-- element setting program-->
        <row_setting_program>Налаштування</row_setting_program>
      </rows_tab>
    </tab_main_menu>
  </menu_bar>
  <!-- Text for error notifications-->
  <error_sector>
    <!-- Module xkbswitch -->
          <xkbswitch_not_fount>
            <!-- Short manual error-->
            <short_description_error>Встановите xkbswitch</short_description_error>
            <!-- Long Manual error-->
            <long_description_error>
             Відкрийте файл README.MD.
              Після цього дотримуйтесь інструкцій установки необхідного модуля у пункті один.
            </long_description_error>
          </xkbswitch_not_fount>
          <!-- Language keyboard error-->
          <language_keyboard>
            <!-- Language keyboard window`s 10-->
            <windows_10>
              <!-- Short manual error-->
              <change_language> Помилка зміни мови клавіатури</change_language>
            </windows_10>
          </language_keyboard>
    <!-- Database error-->
          <database>
            <!-- Label not working sql error-->
            <label_not_working_sql>Не працює sql:</label_not_working_sql>
            <!-- Label value error-->
            <label_value>Помилка:</label_value>
          </database>

  </error_sector>

</data>